  },
  "homepage": "https://github.com/ArtemeeSenin/manning-micro-frontends#readme",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5"
  }
}
